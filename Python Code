x=int(input("Enter your age:"))
#y=0
while x>=18 or x<18:
    if x>=18:
     print("You can vote")
     x=int(input("Enter your age:")) 
    elif (x<=0): 
     print("you need to born first")
     x=int(input("Enter your age:"))
    else:
     print("You are not authorized to give vote")
     x=int(input("Enter your age:")) 
